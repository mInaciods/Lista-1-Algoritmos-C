#include <stdio.h>

int num1, num2, num3;

int main(void) {
  printf("Escreva um numero:");
  scanf("%i", &num1);
  printf("Escreva outro numero:");
  scanf("%i", &num2);
  printf("Escreva um terceiro numero:");
  scanf("%i", &num3);
  float media = (num1 + num2 + num3) / 3;
  printf("A m√©dia aritmeticados numeros e: %f", media);
  return 0;
}

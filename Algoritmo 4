#include <stdio.h>

int num1, num2;

int main(void) {
  printf("Escreva um numero:");
  scanf("%i", &num1);
  printf("Escreva outro numero:");
  scanf("%i", &num2);
  if(num1 > num2){
    printf("O primeiro numero é maior que o segundo: %i", num1);
  } else {
    printf("O segundo é maior: %i", num2);
  }
  return 0;
}
